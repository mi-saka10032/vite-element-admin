{
  "name": "vite-element-admin",
  "version": "1.0.0",
  "description": "A magical vue admin based on vite. An out-of-box UI solution for enterprise applications. Newest development stack of vue. Lots of awesome features",
  "author": "misaka10032@aliyun.com",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --ext .js,.vue src",
    "build:prod": "vite build",
    "build:stage": "vite build --mode staging",
    "preview": "vite preview",
    "new": "plop",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "test:unit": "jest --clearCache && jest",
    "test:ci": "npm run lint && npm run test:unit"
  },
